@model List<VisitorViewModel>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Visitor Api Listesi</h1>
<br />
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Ad</th>
        <th>Soyad</th>
        <th>Ziyaret Sayısı</th>
        <th>Tarih</th>
        <th>Sil</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.VisitorID</td>
            <td>@item.Name</td>
            <td>@item.Surname</td>
            <td>@item.CityVisitCount</td>
            <td>@item.VisitDate</td>
        </tr>
    }
</table>

